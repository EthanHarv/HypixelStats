{
  "name": "hypixelstats",
  "version": "1.0.1",
  "description": "Standalone Cross-platform and open-source Hypixel Stattracker",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "publish": "electron-builder -wl -p always",
    "rebuild": "electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanHarv/HypixelStats.git"
  },
  "keywords": [],
  "author": "Ethanharv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EthanHarv/HypixelStats/issues"
  },
  "homepage": "https://github.com/EthanHarv/HypixelStats#readme",
  "devDependencies": {
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "electron-rebuild": "^2.3.5"
  },
  "dependencies": {
    "chokidar": "^3.5.1",
    "electron-log": "^4.3.2",
    "electron-store": "^7.0.2",
    "electron-updater": "^4.3.8",
    "read-last-lines": "^1.8.0"
  },
  "build": {
    "productName": "Hypixel Stats",
    "artifactName": "HypixelStats.${ext}",
    "appId": "com.ethanharv.hypixelstats",
    "directories": {
      "app": "./",
      "output": "./dist"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "EthanHarv",
        "repo": "HypixelStats"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
